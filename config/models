Item
  apiId Int
  deleted Bool default=False
  itemType ItemType Maybe
  username Text Maybe
  created UTCTime
  text Text Maybe
  dead Bool default=False
  parent Text Maybe
  poll Int Maybe
  url Text
  score Int Maybe
  title Text Maybe
  descendants Int Maybe
  UniqueApiId apiId
  deriving Show Eq Data Typeable Generic

Kid
  itemId ItemId
  apiId Int
  deriving Show

Part
  itemId ItemId
  apiId Int
  deriving Show
